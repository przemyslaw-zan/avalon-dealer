{
	"compilerOptions": {
		"skipLibCheck": true,
		"noImplicitAny": true,
		"noEmit": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true,
		"jsx": "react-jsx"
	},
	"include": [
		"app",
		"server",
		"config.ts"
	]
}
